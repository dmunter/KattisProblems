import java.util.Scanner;


public class reverse {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		 int x = sc.nextInt();

		 int revArr[] = new int[x];

		 while(x>=0) {				 
			 revArr [x--] = sc.nextInt();
			
			 //System.out.print(x);
		 }
		for(int y = 0; y <= revArr.length ;y++) {
			 System.out.print(revArr[y]);
		 }
	}
	 	
}
