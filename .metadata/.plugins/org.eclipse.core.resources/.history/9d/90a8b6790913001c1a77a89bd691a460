import java.util.Scanner;


public class reverse {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		 int x = sc.nextInt();
		 int newInt;

		 int revArr[] = new int[x-1];
		 for(int i=0; i <= x; i++) {		
			 revArr [x-1] = sc.nextInt();
			 //System.out.print(x);
		 }
		for(int y = 0; y <= revArr.length ;y++) {
			 System.out.print(revArr[y]);
		 }
	}
	 	
}
